		{
			"name": "push (fcm)",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "X-AN-APP-NAME",
						"value": "{{app-name}}"
					},
					{
						"key": "X-AN-APP-KEY",
						"value": "{{access-key}}"
					},
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"device\": \"fcm\",\n    \"alert\": {\n        \"title\": \"hello @ {{$timestamp}}\",\n        \"body\": \"From AirNotifier {{$guid}}\"\n    },\n    \"token\": \"{{airnotifier-device-token}}\"\n}"
				},
				"url": {
					"raw": "{{airnotifier-host}}/api/v2/push",
					"host": [
						"{{airnotifier-host}}"
					],
					"path": [
						"api",
						"v2",
						"push"
					]
				}
			},
			"response": []
